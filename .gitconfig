[user]
	name = Luke Gruber
	email = luke.gru@gmail.com
[core]
  	editor = vim
	sharedRepository = 640
	excludesfile = /home/luke/dotfiles/.gitignore
[alias]
	co = checkout
	ci = commit
	st = status -sb
	br = branch
	bra = branch -a
	ds = diff --stat
	dcs = diff --cached --stat
	remotes = remote -v show
  	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  	type = cat-file -t
  	dump = cat-file -p
	cl = clone
	saveass = reflog show HEAD@{now} -10
[color]
	ui = true
[branch]
	autosetupmerge = true
	autosetuprebase = never
[merge]
	summary = true
	stat = true
[mergetool]
	keepBackup = true
[notes]
	rewrite = true
[apply]
	whitespace = warn
[push]
	default = tracking
[github]
	user = luke-gru
[grep]
	extendedRegexp = true
	lineNumber = true
[instaweb]
	httpd = webrick
